
#
# Git configs & extensions
#

[$HOME/.GIT/aladdin/git-config]
stowable = true
checkout = git clone ssh://git@github.com/MagicAladdin/git-config
remotes = git_add_remote upstream ssh://git@github.com/wandsas/git-config
skip = missing_exe git

[$HOME/.GIT/3rd-party/diff-so-fancy]
checkout = git clone git://github.com/so-fancy/diff-so-fancy
skip = true

[$HOME/.GIT/3rd-party/zsh-diff-so-fancy]
checkout = git clone --recurse-submodules git://github.com/zdharma/zsh-diff-so-fancy
skip = true

