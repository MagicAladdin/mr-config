lib =
  mr_stow_target_usr_local
  mr_init_stow_package
fixups =
  if ! [ -e configure ]; then
    autoreconf -i
  fi
  ./configure --prefix=/usr/local
  make install prefix=$STOW_PKG_PATH
  mr_restow_regardless


